
@{ Layout = null; }

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Admin Panel</title>
    <!-- Bootstap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    @*<link href="~/Content/bootstrap.min.css" rel="stylesheet" />*@
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
    <!-- Content time line-->
    <link href="~/AdminLTE-3.0.4/dist/css/timeLine.css" rel="stylesheet" />
    <!-- SweetAlert -->
    <link href="~/AdminLTE-3.0.4/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css" rel="stylesheet" />
    <!-- 404 page -->
    <link href="~/AdminLTE-3.0.4/dist/css/page404.css" rel="stylesheet" />
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/dist/css/adminlte.min.css">
    <!-- Validation-->
    <link href="~/Content/ValidationCSS.css" rel="stylesheet" />
    

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <!-- Datatables -->
    <link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/buttons.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/dtTop.css" rel="stylesheet" />
    <!-- PagedList -->
    <link href="~/Content/PagedList.css" rel="stylesheet" />

</head>

<body class="hold-transition sidebar-mini">
    <div class="wrapper">

        <aside class="main-sidebar sidebar-dark-primary elevation-4">

            <a href="#" class="brand-link">
                <img src="~/AdminLTE-3.0.4/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">AdminPanel</span>
            </a>

            <div class="sidebar">

                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/AdminLTE-3.0.4/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">

                        <a href="#" class="d-block">@Html.Action("GetLoggedNameSurname", "WriterLogin")</a>
                    </div>
                </div>

                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                        <li class="nav-item">
                            <a href="/AdminCategory/Index/" class="nav-link">
                                <i class="nav-icon fas fa-layer-group"></i>
                                <p>
                                    Categories
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-book-reader"></i>
                                <p>
                                    Entries
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-tenge"></i>
                                <p>
                                    Titles
                                </p>
                                <i class="right fas fa-angle-left"></i>

                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Heading/Index" class="nav-link">
                                        <i class="fas fa-user-check nav-icon"></i>
                                        <p> Active Titles</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Heading/GetPassiveHeadings/" class="nav-link">
                                        <i class="fas fa-user-times nav-icon"></i>
                                        <p> Passive Titles</p>
                                    </a>
                                </li>

                            </ul>
                        </li>

                        <li class="nav-item has-treeview">
                            <a href="" class="nav-link">
                                <i class="nav-icon fas fa-feather-alt"></i>
                                <p>
                                    Writers
                                </p>
                                <i class="right fas fa-angle-left"></i>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Writer/Index" class="nav-link">
                                        <i class="fas fa-user-check nav-icon"></i>
                                        <p> Active Writers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Writer/GetBannedWriters/" class="nav-link">
                                        <i class="fas fa-user-times nav-icon"></i>
                                        <p> Banned Writers</p>
                                    </a>
                                </li>

                            </ul>
                        </li>


                        <li class="nav-item has-treeview">
                            <a href="/Charts/Index" class="nav-link">
                                <i class="nav-icon fas fa-chart-pie"></i>
                                <p>
                                    Graphics

                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/About/Index/" class="nav-link">
                                <i class="nav-icon fas fa-users"></i>
                                <p>
                                    About Us
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Message/Inbox/" class="nav-link">
                                <i class="nav-icon fas fa-envelope"></i>
                                <p>
                                    Contact & Messages
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="pages/calendar.html" class="nav-link">
                                <i class="nav-icon fas fa-user-tie"></i>
                                <p>
                                    Authorizations
                                </p>
                            </a>
                        </li>



                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-exclamation-triangle"></i>
                                <p>
                                    Error Pages

                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-step-forward"></i>
                                <p>
                                    Go to the website
                                </p>
                            </a>

                        </li>
                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-sign-out-alt"></i>
                                <p>
                                    Logout
                                </p>
                            </a>

                        </li>
                        <hr />
                        <li class="nav-item has-treeview">
                            <a href="/ToDoList/Index/" class="nav-link">
                                <i class="nav-icon fab fa-angellist"></i>
                                <p>
                                    ToDoList - Backend
                                </p>
                                <i class="nav-icon fab fa-angellist"></i>
                            </a>

                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/ToDoList/IndexFront/" class="nav-link">
                                <i class="nav-icon fab fa-angellist"></i>
                                <p>
                                    ToDoList - Frontend
                                </p>
                                <i class="nav-icon fab fa-angellist"></i>
                            </a>

                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <div class="content">
                @RenderBody()
                <!-- /.container-fluid -->
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <!-- jQuery Ajax -->
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- validation client side -->
    @RenderSection("scripts", false)


    <!-- SweetAlert-->
    <script src="~/AdminLTE-3.0.4/plugins/sweetalert2/sweetalert2.min.js"></script>
    <!-- AdminLTE-->
    <script src="~/AdminLTE-3.0.4/dist/js/adminlte.js"></script>
    <!-- OPTIONAL SCRIPTS -->
    <script src="~/AdminLTE-3.0.4/plugins/chart.js/Chart.min.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/demo.js"></script>
    <script src="~/AdminLTE-3.0.4/dist/js/pages/dashboard3.js"></script>
    <!-- Datatable-->
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Content/DTScripts.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/jszip/jszip.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.buttons.min.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/AdminLTE-3.0.4/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/Scripts/DataTables/buttons.html5.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.print.min.js"></script>
    <!-- ajax deleete-->
    <script src="~/Content/AllCrudsAJAX.js"></script>

</body>
</html>
