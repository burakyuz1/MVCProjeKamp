@model List<EntityLayer.Concrate.Content>

@{
    ViewBag.Title = "GetContents";
    Layout = "~/Views/Shared/_WriterLayout.cshtml";
}

<h3>All texts for @Model[0].Heading.HeadingName</h3>
<div class="tracking-list">

    @foreach (var content in Model)
    {
        <div class="tracking-item">
            <div class="tracking-icon status-intransit">
                <i class="fa fa-app-store-ios"></i>
                @*<a>
                    <svg class="svg-inline--fa fa-circle fa-w-16" aria-hidden="true" data-prefix="fas" data-icon="circle" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" data-fa-i2svg="">
                        <path fill="currentColor" d="M256 8C119 8 8 119 8 256s111 248 248 248 248-111 248-248S393 8 256 8z"></path>
                    
                    </svg>
                </a>*@
                <!-- <i class="fas fa-circle"></i> -->
            </div>
            <div class="tracking-date"><span class="bg-secondary">@(((DateTime)content.ContentDate).ToString("dd-MM-yyyy"))</span><span class="bg-secondary">@(((DateTime)content.ContentDate).ToString("HH : mm"))</span></div>
            <div class="tracking-content">@content.Heading.HeadingName <span>@content.ContentText</span></div>
        </div>

    }


</div>


